@model IEnumerable<SFBCPanel.Models.UsersMangementViewModel>

@{
    ViewBag.Title = "View Report";
}

<header>

    @*<script src="~/Content/jquery-1.11.3.min.js"></script>*@
    <script src="~/Content/jquery.dataTables.js"></script>
    <link href="~/Content/jquery.dataTables.css" rel="stylesheet" />

    <script>
        $(document).ready(function () {
            var table = $('#example').DataTable();
        });
    </script>
</header>

<section class="content">
    <div class="row">
        <div class="col-md-12">
            <!-- TABLE: USerLog -->
            <div class="box box-info">
                <h2>Users Log</h2><br />
                <ol class="breadcrumb" style="font-size:medium">
                    Users Log Report
                </ol>
                <div class="box-body">
                    <div class="col-lg-12 col-md-11 col-sm-11">
                        <table id="example" class="display nowrap" style="font-size:small">
                            <thead>
                                <tr>
                                    <th style="font-size:small">
                                        @Html.DisplayNameFor(model => model.IpAddress)
                                    </th>
                                    <th style="font-size:small">
                                        @Html.DisplayNameFor(model => model.LoginTime)
                                    </th>
                                    <th style="font-size:small">
                                        @Html.DisplayNameFor(model => model.UserPass)
                                    </th>
                                    <th style="font-size:small">
                                        @Html.DisplayNameFor(model => model.UserLogin)
                                    </th>
                                    <th style="font-size:small">
                                        @Html.DisplayNameFor(model => model.LoginStatus)
                                    </th>
                                </tr>
                            </thead>

                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td style="font-size:small">
                                            @Html.DisplayFor(modelItem => item.IpAddress)
                                        </td>
                                        <td style="font-size:small">
                                            @Html.DisplayFor(modelItem => item.LoginTime)
                                        </td>
                                        <td style="font-size:small">
                                            @Html.DisplayFor(modelItem => item.UserPass)
                                        </td>
                                        <td style="font-size:small">
                                            @Html.DisplayFor(modelItem => item.UserLogin)
                                        </td>
                                        <td style="font-size:small">
                                            @Html.DisplayFor(modelItem => item.LoginStatus)
                                        </td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
